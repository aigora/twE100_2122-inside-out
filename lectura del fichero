#include <stdio.h>
#define F 6 
#define C 6 
int main()
{
int i, j, n;
int matriz[F][C];
FILE *pf;
// Abrimos fichero para lectura
pf = fopen("Laberinto 1.txt", "r");
if (pf==NULL)
{
    printf("No se ha podido abrir el fichero");
}
else
{

// Leemos datos separados por comas
for ( i = 0; i < F; i++)
{
    for ( j = 0; i < C; j++)
    {
       fscanf(pf, "%i", &matriz[i][j]);
    }
    
}
fclose(pf);
// Mostramos en pantalla lo leÃ­do
for ( i = 0; i < F; i++)
{
    for ( j = 0; i < C; j++)
    {
       printf("%i", matriz[i][j]);
    }
    printf("\n");
}
}

return(0);
}
